jQuery( document ).ready( function($) {
    var body = $( 'body' );
    if ( body.hasClass( 'blog' ) || body.hasClass( 'archive' ) ) {
        $( '.type-post' ).on( 'click', function() {
            var link = $( this ).find( '.entry-title a' ).attr( 'href' );
            if ( link ) {
                window.location = link;
            }
        } );
    }
} );
